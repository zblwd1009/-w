Tab:AddButton({	Name = "自动挥拳",	Callback = function()while true do
local args = {
	"punch",
	"rightHand"
}
game:GetService("Players").LocalPlayer:WaitForChild("muscleEvent"):FireServer(unpack(args))
wait(0.5)
end})    

